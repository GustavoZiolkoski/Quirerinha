<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Quirerinha.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Quirerinha"
    xmlns:local2="clr-namespace:Quirerinha.Views"    
    Shell.FlyoutBehavior="Disabled"
    Title="Quirerinha">

    <ShellContent
    Title="Inicio" 
    FlyoutItemIsVisible="true" 
    Shell.FlyoutBehavior="Flyout"
    ContentTemplate="{DataTemplate local2:Home}"
    Route="Home" />
    <ShellContent FlyoutItemIsVisible="true" 
    Shell.FlyoutBehavior="Flyout" Title="Configurações" ContentTemplate="{DataTemplate local2:PaginaPrincipal}" Route="Config"/>
    <ShellContent FlyoutItemIsVisible="true" 
    />

</Shell>
